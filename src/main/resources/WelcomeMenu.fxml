<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxWidth="1.7976931348623157E308" prefHeight="350.0" prefWidth="600.0"
            style="-fx-background-color: #e6e6e6;" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.galaxyschool.view.WelcomeMenu">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="225.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@schoolLogo.png" />
         </image>
      </ImageView>
      <Button fx:id="TeacherB" layoutX="178.0" layoutY="200.0" mnemonicParsing="false" onAction="#showTeacherPanel" prefHeight="35.0" prefWidth="95.0" style="-fx-background-color: #00A2D3;" text="I'm a Teacher" textFill="WHITE">
         <font>
            <Font name="Candara Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="StudentB" layoutX="328.0" layoutY="200.0" mnemonicParsing="false" onAction="#showStudentPanel" prefHeight="35.0" prefWidth="95.0" style="-fx-background-color: #00A2D3;" text="I'm a Student" textFill="WHITE">
         <font>
            <Font name="Candara Bold" size="12.0" />
         </font>
      </Button>
      <HBox alignment="TOP_RIGHT" maxWidth="1.7976931348623157E308" prefHeight="35.0" prefWidth="614.0" style="-fx-background-color: #004080;">
         <children>
            <ImageView fx:id="minimizeB" fitHeight="18.0" fitWidth="160.0" layoutX="592.0" layoutY="10.0" nodeOrientation="RIGHT_TO_LEFT" onMouseClicked="#hideWindow" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@minimizeB.png" />
               </image>
            </ImageView>
            <ImageView fx:id="closeB" fitHeight="18.0" fitWidth="160.0" layoutX="592.0" layoutY="10.0" nodeOrientation="RIGHT_TO_LEFT" onMouseClicked="#closeWindow" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@closeB.png" />
               </image>
            </ImageView>
         </children></HBox>
   </children>
</AnchorPane>
